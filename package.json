{
  "name": "configcat-vue",
  "type": "module",
  "version": "1.0.1",
  "description": "A simple and easy to use Vue3 plugin for using ConfigCat Feature Flags in your Vue.js Application",
  "keywords": [
    "ConfigCat",
    "vue feature flags",
    "vuejs feature flags",
    "feature management",
    "feature flags",
    "configcat vue"
  ],
  "author": "Chavez Harris",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "./dist/configcat-vue.umd.cjs",
  "module": "./dist/configcat-vue.js",
  "exports": {
    ".": {
      "import": "./dist/configcat-vue.js",
      "require": "./dist/configcat-vue.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "configcat-js": "^8.0.2",
    "vite": "^4.0.0"
  }
}
